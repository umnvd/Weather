<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CircleDiagramView">
        <attr name="value" format="integer" />
        <attr name="filledColor" format="color" />
        <attr name="blankColor" format="color" />
        <attr name="textColor" format="color" />
        <attr name="clipColor" format="color" />
        <attr name="textSize" format="dimension" />
        <attr name="arcWidth" format="dimension" />
    </declare-styleable>

    <attr name="circleDiagramStyle" format="reference" />

    <style name="CircleDiagramDefaultStyle">
        <item name="filledColor">#7E57C2</item>
        <item name="blankColor">#1A7E57C2</item>
        <item name="textColor">?android:attr/textColorPrimary</item>
        <item name="clipColor">?android:attr/colorBackground</item>
    </style>

</resources>